msgstr "Zugriffs-Token fehlt"

msgid "Invalid access token"
msgstr "Ungültiges Zugriffs-Token"

msgid "User not found"
msgstr "Benutzer nicht gefunden"

msgid "Client ID mismatch"
msgstr "Client-ID stimmt nicht überein"

msgid "Invalid post_logout_redirect_uri"
msgstr "Ungültige Weiterleitungs-URI nach Abmeldung"

msgid "Failed to generate authorization code"
msgstr "Autorisierungscode konnte nicht generiert werden"

msgid "OAuth2 Error"
msgstr "OAuth2-Fehler"

msgid "Successfully logged out"
msgstr "Erfolgreich abgemeldet"
