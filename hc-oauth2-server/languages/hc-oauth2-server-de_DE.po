msgid "OAuth2 Server"
msgstr "OAuth2-Server"

msgid "Auth Clients"
msgstr "Authentifizierungs-Clients"

msgid "Auth Tokens"
msgstr "Authentifizierungs-Tokens"

msgid "Webhook Settings"
msgstr "Webhook-Einstellungen"

msgid "OAuth2 client has been successfully added."
msgstr "Der OAuth2-Client wurde erfolgreich hinzugefügt."

msgid "OAuth2 client has been successfully deleted."
msgstr "Der OAuth2-Client wurde erfolgreich gelöscht."

msgid "Client secret has been successfully regenerated."
msgstr "Das Client-Geheimnis wurde erfolgreich neu generiert."

msgid "Access token has been successfully revoked."
msgstr "Das Zugriffs-Token wurde erfolgreich widerrufen."

msgid "Settings have been successfully saved."
msgstr "Die Einstellungen wurden erfolgreich gespeichert."

msgid "Error: Missing required fields. Please fill in all required information."
msgstr "Fehler: Erforderliche Felder fehlen. Bitte füllen Sie alle erforderlichen Informationen aus."

msgid "Error: Failed to add client. Please try again."
msgstr "Fehler: Client konnte nicht hinzugefügt werden. Bitte versuchen Sie es erneut."

msgid "Error: Failed to delete client. Please try again."
msgstr "Fehler: Client konnte nicht gelöscht werden. Bitte versuchen Sie es erneut."

msgid "Error: Failed to regenerate client secret. Please try again."
msgstr "Fehler: Client-Geheimnis konnte nicht neu generiert werden. Bitte versuchen Sie es erneut."

msgid "Error: Failed to revoke token. Please try again."
msgstr "Fehler: Token konnte nicht widerrufen werden. Bitte versuchen Sie es erneut."

msgid "Error: Failed to save settings. Please try again."
msgstr "Fehler: Einstellungen konnten nicht gespeichert werden. Bitte versuchen Sie es erneut."

msgid "Error: Security check failed. Please try again."
msgstr "Fehler: Sicherheitsüberprüfung fehlgeschlagen. Bitte versuchen Sie es erneut."

