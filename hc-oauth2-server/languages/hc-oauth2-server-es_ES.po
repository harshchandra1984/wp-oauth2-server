msgid "OAuth2 Server"
msgstr "Servidor OAuth2"

msgid "Auth Clients"
msgstr "Clientes de Autenticación"

msgid "Auth Tokens"
msgstr "Tokens de Autenticación"

msgid "Webhook Settings"
msgstr "Configuración de Webhooks"

msgid "OAuth2 client has been successfully added."
msgstr "El cliente OAuth2 se ha agregado correctamente."

msgid "OAuth2 client has been successfully deleted."
msgstr "El cliente OAuth2 se ha eliminado correctamente."

msgid "Client secret has been successfully regenerated."
msgstr "El secreto del cliente se ha regenerado correctamente."

msgid "Access token has been successfully revoked."
msgstr "El token de acceso se ha revocado correctamente."

msgid "Settings have been successfully saved."
msgstr "La configuración se ha guardado correctamente."

msgid "Error: Missing required fields. Please fill in all required information."
msgstr "Error: Faltan campos requeridos. Por favor, complete toda la información requerida."

msgid "Error: Failed to add client. Please try again."
msgstr "Error: No se pudo agregar el cliente. Por favor, intente de nuevo."

msgid "Error: Failed to delete client. Please try again."
msgstr "Error: No se pudo eliminar el cliente. Por favor, intente de nuevo."

msgid "Error: Failed to regenerate client secret. Please try again."
msgstr "Error: No se pudo regenerar el secreto del cliente. Por favor, intente de nuevo."

msgid "Error: Failed to revoke token. Please try again."
msgstr "Error: No se pudo revocar el token. Por favor, intente de nuevo."

msgid "Error: Failed to save settings. Please try again."
msgstr "Error: No se pudieron guardar la configuración. Por favor, intente de nuevo."

msgid "Error: Security check failed. Please try again."
msgstr "Error: La verificación de seguridad falló. Por favor, intente de nuevo."

msgid "Server Information"
msgstr "Información del Servidor"

msgid "Authorization Endpoint"
msgstr "Endpoint de Autorización"

msgid "Token Endpoint"
msgstr "Endpoint de Token"

msgid "Userinfo Endpoint"
msgstr "Endpoint de Información de Usuario"

msgid "Logout Endpoint"
msgstr "Endpoint de Cierre de Sesión"

msgid "Quick Stats"
msgstr "Estadísticas Rápidas"

msgid "Registered Clients:"
msgstr "Clientes Registrados:"

